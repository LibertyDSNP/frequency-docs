{
  "excludeFiles": [
    ".git",
    "yaspeller",
    "node_modules",
    "LICENSE.md"
  ],
  "format": "markdown",
  "lang": "en",
  "fileExtensions": [".md"],
  "report": ["console"],
  "ignoreCapitalization": true,
  "ignoreDigits": true,
  "ignoreRomanNumerals": true,
  "ignoreTags": ["code", "script"],
  "ignoreUppercase": true,
  "ignoreUrls": true,
  "maxRequests": 5,
  "dictionary": [
    "GitHub",
    "Mainnet",
    "Roadmap",
    "Screenshots",
    "Testnet",
    "Tokenomics",
    "mdBook",
    "parachain",
    "parachains",
    "repo",
    "xyz",
    "markdown",
    "polkadot",
    "MSAs",
    "Alexa",
    "blockchain",
    "delegator",
    "payload",
    "validator",
    "unstaking"
  ]
}
